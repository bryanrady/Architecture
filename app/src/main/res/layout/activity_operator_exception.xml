<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true">

    <include layout="@layout/toolbar_layout"/>

    <Button
        android:id="@+id/btn_exception_operator_onErrorReturn"
        android:layout_below="@+id/toolbar"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_centerHorizontal="true"
        android:textAllCaps="false"
        android:text="onErrorReturn" />

    <Button
        android:id="@+id/btn_exception_operator_onExceptionResumeNext"
        android:layout_below="@+id/btn_exception_operator_onErrorReturn"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_centerHorizontal="true"
        android:textAllCaps="false"
        android:text="onExceptionResumeNext" />

    <Button
        android:id="@+id/btn_exception_operator_onErrorResumeNext"
        android:layout_below="@+id/btn_exception_operator_onExceptionResumeNext"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_centerHorizontal="true"
        android:textAllCaps="false"
        android:text="onErrorResumeNext" />


</RelativeLayout>
