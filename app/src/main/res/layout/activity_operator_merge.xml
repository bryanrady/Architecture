<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true">

    <include layout="@layout/toolbar_layout"/>

    <Button
        android:id="@+id/btn_merge_operator_startWith"
        android:layout_below="@+id/toolbar"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_centerHorizontal="true"
        android:textAllCaps="false"
        android:text="startWith" />

    <Button
        android:id="@+id/btn_merge_operator_concat"
        android:layout_below="@+id/btn_merge_operator_startWith"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_centerHorizontal="true"
        android:textAllCaps="false"
        android:text="concat" />

    <Button
        android:id="@+id/btn_merge_operator_merge"
        android:layout_below="@+id/btn_merge_operator_concat"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_centerHorizontal="true"
        android:textAllCaps="false"
        android:text="merge" />

    <Button
        android:id="@+id/btn_merge_operator_xxMapDelayError"
        android:layout_below="@+id/btn_merge_operator_merge"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_centerHorizontal="true"
        android:textAllCaps="false"
        android:text="xxMapDelayError" />

    <Button
        android:id="@+id/btn_merge_operator_zip"
        android:layout_below="@+id/btn_merge_operator_xxMapDelayError"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_centerHorizontal="true"
        android:textAllCaps="false"
        android:text="zip" />

</RelativeLayout>
