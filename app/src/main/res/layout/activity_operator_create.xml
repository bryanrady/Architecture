<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true">

    <include layout="@layout/toolbar_layout"/>

    <Button
        android:id="@+id/btn_create_operator_create"
        android:layout_below="@+id/toolbar"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_centerHorizontal="true"
        android:textAllCaps="false"
        android:text="create" />

    <Button
        android:id="@+id/btn_create_operator_just"
        android:layout_below="@+id/btn_create_operator_create"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_centerHorizontal="true"
        android:textAllCaps="false"
        android:text="just" />

    <Button
        android:id="@+id/btn_create_operator_fromArray"
        android:layout_below="@+id/btn_create_operator_just"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_centerHorizontal="true"
        android:textAllCaps="false"
        android:text="fromArray" />

    <Button
        android:id="@+id/btn_create_operator_emty"
        android:layout_below="@+id/btn_create_operator_fromArray"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_centerHorizontal="true"
        android:textAllCaps="false"
        android:text="empty" />

    <Button
        android:id="@+id/btn_create_operator_defer"
        android:layout_below="@+id/btn_create_operator_emty"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_centerHorizontal="true"
        android:textAllCaps="false"
        android:text="defer" />

    <Button
        android:id="@+id/btn_create_operator_interval"
        android:layout_below="@+id/btn_create_operator_defer"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_centerHorizontal="true"
        android:textAllCaps="false"
        android:text="interval" />

</RelativeLayout>
